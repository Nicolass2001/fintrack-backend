  "license": "ISC",
  "description": "",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.21.1",
    "bcrypt": "5.1.1",
    "cors": "2.8.5",

    "swagger-ui-express": "^5.0.1",
    "zod": "3.24.1"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@types/bcrypt": "5.0.2",
    "@types/cors": "2.8.17",
    "@types/dotenv": "6.1.1",
